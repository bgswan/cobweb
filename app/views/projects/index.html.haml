.container
  - if @postcode
    %h1 Projects near #{ @postcode }

  .row
    %table.table.table-striped
      %tr
        %th Project Name
        %th Location
        %th # of Properties
        %th Running since
        %th Join
      = render @projects
  .row
    %p.lead
      Or create a new project:
    .well
      = render partial: 'projects/form'
